declare sub IhopSlag(ByVal typ as string,ByVal typ2 as string, path as string)
		
sub IhopSlag(ByVal typ as string,ByVal typ2 as string, path as string)
		dim tablenames(3) as string

		tablenames(1) = typ & "Mellersta"
		tablenames(2) = typ & "Norra"
		tablenames(3) = typ & "S�dra"
		call tableOpener(tablenames)	

		call insertXintoYtableWhereXcolLessThaYcol(tablenames(1),typ2)
		call insertXintoYtableWhereXcolLessThaYcol(tablenames(2),typ2)
		call insertXintoYtableWhereXcolLessThaYcol(tablenames(3),typ2)
		Commit table reningIhopslagen
		
		call UpdateIndex(typ2)'already commits
		insert into Pathtable (tablePath,TableName) values (path, typ2)	
		call tableCloserAndCommiter(tableNames)
end sub
 
